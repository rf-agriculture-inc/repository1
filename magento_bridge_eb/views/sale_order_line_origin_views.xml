<odoo>
    <data>

        <record id="sale_order_line_origin_view_form" model="ir.ui.view">
            <field name="name">sale.order.line.origin form view</field>
            <field name="model">sale.order.line.origin</field>
            <field name="arch" type="xml">
                <form>
                    <sheet>
                        <group>
                            <group name="group_1">
                                <field name="product_id"/>
                                <field name="product_uom_qty"/>
                                <field name="price_unit"/>
                                <field name="tax_id" widget="many2many_tags"/>
                                <field name="discount"/>
                            </group>
                            <group name="group_2">
                                <field name="name"/>
                                <field name="mag_quote_id"/>
                                <field name="mag_id"/>
                            </group>
                        </group>
                    </sheet>
                </form>
            </field>
        </record>

    </data>
</odoo>