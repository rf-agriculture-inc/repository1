<odoo>
    <data noupdate="1">

        <record id="mag_update_product_prices" model="ir.cron">
            <field name="name">Magento Update Product Prices</field>
            <field name="model_id" ref="magento_bridge_eb.model_magento_bridge_config"/>
            <field name="state">code</field>
            <field name="code">model.update_product_prices()</field>
            <field name="interval_number">10</field>
            <field name="interval_type">minutes</field>
            <field name="numbercall">-1</field>
            <field name="active" eval="False" />
            <field name="doall" eval="True" />
        </record>

    </data>
</odoo>